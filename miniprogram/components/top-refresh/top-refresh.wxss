/* components/top-refresh/top-refresh.wxss */
.refresh {
  position: absolute;
  width: 2rem;
  height: 2rem;
  border-radius: 50%;
  left: 50%;
  margin-top: 3rem;
  z-index: 888;
}
.refresh>image {
  width: 100%;
  height: 100%;
}

